{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 5 versions", "not ie <= 8"]
      }
    }],
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false, 
      "polyfill": true,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": [["env"], "react"],
      "plugins": [
        ["transform-runtime", {
          "helpers": false, 
          "polyfill": true,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }],
        "syntax-dynamic-import",
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    }
  }
}